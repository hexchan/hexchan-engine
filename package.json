{
  "name": "hexchan",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build:styles": "npx lessc --source-map --math=strict src/imageboard/static/imageboard/style.less src/imageboard/static/imageboard/style.css",
    "build:scripts": "npx rollup --sourcemap --silent --config src/imageboard/static/imageboard/rollup.config.js",
    "watch:styles": "npx chokidar \"src/imageboard/static/imageboard/styles/**/*.less\" -c \"npm run build:styles\"",
    "watch:scripts": "npx chokidar \"src/imageboard/static/imageboard/scripts/**/*.js\" -c \"npm run build:scripts\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "material-design-icons": "^3.0.1"
  },
  "devDependencies": {
    "chokidar": "^3.4.1",
    "chokidar-cli": "^2.1.0",
    "less": "^3.11.1",
    "rollup": "^1.32.1"
  }
}
